<!DOCTYPE html>
<html>
<head>
	<title></title>
</head>
<body>
	<div class="span8 welcome-image">
    <div id="myCarousel" class="carousel slide" data-ride="carousel">
      <!-- Indicators -->
      <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" ></li>
        <li data-target="#myCarousel" data-slide-to="1" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
      </ol>

      <!-- Wrapper for slides -->
      <div class="carousel-inner">
        <div class="item">
          <a href="/cities/London"><img src="http://i.imgur.com/mJIHxSi.jpg" alt="London">
          <div class="carousel-caption d-none d-md-block">
        <h1 class="carousel-header">LONDON</h1>
        <p class="carousel-body">UNITED KINGDOM</p>
        </div>
        </a>
        </div>

        <div class="item active">
          <a href="/cities/Atlanta"><img src="http://i.imgur.com/TnuIg5U.jpg" alt="Atlanta">
        <div class="carousel-caption d-none d-md-block">
        <h1 class="carousel-header">ATLANTA</h1>
        <p class="carousel-body">GEORGIA</p>
      </div>
      </a>
      </div>
      
        <div class="item">
          <a href="/cities/San Francisco"><img src="http://i.imgur.com/PCGyG1O.jpg" alt="San Francisco">
        <div class="carousel-caption d-none d-md-block">
        <h1 class="carousel-header">SAN FRANCISCO</h1>
        <p class="carousel-body">CALIFORNIA</p>
      </div>
      </a>
    </div>

      <!-- Left and right controls -->
      <a class="left carousel-control" href="#myCarousel" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left"></span>
        <span class="sr-only">Previous</span>
      </a>
      <a class="right carousel-control" href="#myCarousel" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right"></span>
        <span class="sr-only">Next</span>
      </a>
    </div>

</body>
</html>
