<h1><%= "User's Followed By #{@user.username}:"%></h1>

<div class="container">
  <div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
      
      <ul class="list-group">
        <% @users.each do |user| %>
          <li class="list-group-item">
            <%= link_to user.username, user_show_path(user) %>
          </li>
        <% end %>
      </ul>
      
    </div>
  </div>
</div>